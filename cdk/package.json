{
  "name": "dailyreport-cdk",
  "version": "1.0.0",
  "description": "AWS CDK Infrastructure for DailyReport System",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy --all --require-approval never",
    "deploy:dev": "cdk deploy DailyReportStack-dev --require-approval never",
    "deploy:prod": "cdk deploy DailyReportStack-prod --require-approval never",
    "destroy": "cdk destroy --all",
    "synth": "cdk synth",
    "diff": "cdk diff"
  },
  "keywords": [
    "aws",
    "cdk",
    "dailyreport",
    "lambda",
    "rds",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.114.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.114.0",
    "constructs": "^10.3.0",
    "dotenv": "^16.3.1"
  }
}
